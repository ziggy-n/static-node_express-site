extends layout.pug

block content 
  article.grid-container.portfolio-intro
    div
 
      h1 MY PORTFOLIO


      p.lead.text-light
        | This portfolio should give interested parties an insight into work I have done in the past
  article.grid-container.portfolio-index
    
    .grid-x.grid-margin-x.small-up-2.medium-up-2.large-up-3

      each val in projects
        .cell
          a(href=`project_${val.id}`)
              img.thumbnail(src=`static/${val.imageloc}_1.png`)
              h5 #{val.project_name} 
